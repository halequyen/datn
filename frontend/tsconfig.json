{
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["socket.io-client", "element-plus"],
    "ignoreDeprecations": "5.0",
    "lib": ["es2017", "dom"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
